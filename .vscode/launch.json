{
    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Debug Local",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/src/main.go",
            "env": {},
            "args": []
        },
        {
            "name": "Debug Docker Container",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "/app",
            "port": 2345,
            "host": "127.0.0.1",
            "showLog": true
        }
    ]
}